{
	"entryPoints": [
		"example.ts"
	],
	"tsconfig": "tsconfig.json",
	"plugin": [
		"../dist/index.js"
	],
	"katex": {
		"version": "0.11.1",
		"options": {
			"delimiters": [
				{
					"left": "$$",
					"right": "$$",
					"display": true
				},
				{
					"left": "$",
					"right": "$",
					"display": false
				}
			],
			"macros": {
				"\\f": "#1f(#2)",
				"\\space": "\\ ",
				"\\thinSpace": "\\,",
				"\\negSpace": "\\!",
				"\\percent": "\\%",
				"\\lCurl": "\\{",
				"\\rCurl": "\\}",
				"\\lt": "<",
				"\\gt": ">",
				"\\underscore": "_{#1}",
				"\\newline": "\\\\",
				"\\einstein": "E=mc^2"
			}
		}
	},
	"out": "../public"
}
